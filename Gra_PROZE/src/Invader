
public class Invader
{
	Integer x, y, zycie, rzad, kolumna;
	boolean widoczny;
	
	void isVisible(boolean b)
	{
		widoczny = b;
	}
	
	public Invader(){}
	
	public Invader(int x, int y)
	{
		this.x = x;
		this.y = y;
		widoczny = true;	
		zycie = 2;
		kolumna = 0;
		rzad = 0;
	}
	
	
	public static void main(String[] args)
	{
		// TODO Auto-generated method stub

	}

}
